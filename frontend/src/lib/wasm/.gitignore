# Ignore WASM binary files
*.wasm
*_bg.wasm.d.ts

# But allow essential worker files and their types
!worker-load.js
!worker-load.d.ts

# Allow TypeScript source files (already tracked)
!*.ts
!*.d.ts
!*.js

# Allow test files and documentation
!*.test.ts
!*.md

# Allow package.json and gitignore
!package.json
!.gitignore