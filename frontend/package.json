{
	"name": "vec2art-frontend",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"license": "SEE LICENSE IN ../LICENSE",
	"author": "Joshua McDonald",
	"description": "Professional Image-to-SVG Vectorization Web Application",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/vec2art"
	},
	"scripts": {
		"dev": "node scripts/copy-wasm.js && vite dev",
		"build": "npm run build:vercel",
		"build:vercel": "node scripts/copy-wasm.js && npm run build:frontend-only",
		"build:clean": "npm run build:wasm && vite build && echo Build completed successfully",
		"build:frontend-only": "vite build",
		"build:ci": "node scripts/copy-wasm.js && vite build && echo 'Build completed successfully'",
		"copy:wasm": "node scripts/copy-wasm.js",
		"build:wasm": "npm run generate:types && cd .. && node scripts/rebuild-wasm.js",
		"build:wasm:no-types": "cd .. && node scripts/rebuild-wasm.js",
		"build:wasm:clean": "cd .. && node scripts/rebuild-wasm.js --clean",
		"build:wasm:dual": "npm run generate:types && cd .. && node scripts/rebuild-wasm-dual.js",
		"build:wasm:dual:clean": "cd .. && node scripts/rebuild-wasm-dual.js --clean",
		"build:wasm:bash": "cd .. && bash ./scripts/rebuild-wasm-enhanced.sh",
		"build:wasm:ps": "cd .. && pwsh -File ./scripts/rebuild-wasm.ps1",
		"build:wasm:safe": "npm run generate:types:safe && cd .. && node scripts/rebuild-wasm.js || echo 'WASM build failed, using existing artifacts'",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:unit": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest run --coverage --ui",
		"test:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:ui": "playwright test --ui",
		"rebuild-wasm": "cd .. && (./rebuild-wasm.sh || rebuild-wasm.bat)",
		"generate-types": "bash scripts/generate-types.sh",
		"generate:types": "cd ../wasm/vectorize-wasm && cargo test --features generate-ts --lib --target x86_64-pc-windows-msvc || echo 'Type generation completed (some tests failed but types were generated)'",
		"generate:types:safe": "cd ../wasm/vectorize-wasm && cargo test --features generate-ts --lib 2>/dev/null || echo 'Type generation completed (using existing types)'",
		"test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
		"test:smoke": "playwright test tests/e2e/core-workflow.spec.ts --project=chromium",
		"test:ci": "npm run test:coverage && npm run test:e2e",
		"test:ci:fast": "npm run test:unit && npm run test:smoke",
		"test:ci:full": "npm run test:coverage && npm run test:e2e && npm run test:accessibility",
		"test:all": "npm run lint && npm run check && npm run test:ci:full",
		"test:algorithms": "node tests/algorithm-validation/run-validation.js",
		"test:wasm-introspect": "node tests/algorithm-validation/wasm-introspection.js",
		"test:generate-images": "node tests/algorithm-validation/generate-test-images.js",
		"test:install": "playwright install --with-deps",
		"test:simple": "playwright test --config=playwright-simple.config.ts",
		"test:simple:headed": "playwright test --config=playwright-simple.config.ts --headed",
		"test:simple:ui": "playwright test --config=playwright-simple.config.ts --ui",
		"test:report": "playwright show-report",
		"test:report:simple": "playwright show-report playwright-report-simple",
		"test:trace": "playwright show-trace",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"format": "prettier --write .",
		"quality:check": "npm run lint && npm run check && npm run test:coverage",
		"quality:full": "npm run lint && npm run check && npm run test:ci:full",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"storybook:test": "test-storybook",
		"storybook:test:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook:build && npx http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && npm run storybook:test\"",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.11.0",
		"@chromatic-com/storybook": "^4.1.3",
		"@csstools/postcss-oklab-function": "^4.0.12",
		"@playwright/test": "^1.57.0",
		"@storybook/addon-a11y": "^9.1.17",
		"@storybook/addon-docs": "^9.1.17",
		"@storybook/addon-svelte-csf": "^5.0.7",
		"@storybook/addon-vitest": "^9.1.17",
		"@storybook/sveltekit": "^9.1.17",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/eslint-config": "^8.3.4",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.3",
		"@vitest/browser": "4.0.16",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"autoprefixer": "^10.4.23",
		"eslint-plugin-storybook": "^9.1.17",
		"eslint-plugin-svelte": "^3.13.1",
		"happy-dom": "^20.0.11",
		"playwright": "^1.57.0",
		"postcss": "^8.5.6",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"sharp": "^0.34.5",
		"storybook": "^9.1.17",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"@eslint/js": "^9.39.2",
		"@floating-ui/dom": "^1.7.4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@typescript-eslint/parser": "^8.50.0",
		"@vercel/analytics": "^1.6.1",
		"bits-ui": "^2.14.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dexie": "^4.2.1",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.5.0",
		"jsdom": "^27.3.0",
		"lucide-svelte": "^0.561.0",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte-image-viewer": "^6.0.0",
		"svelte-portal": "^2.2.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"vectorize-wasm": "file:../wasm/vectorize-wasm/pkg",
		"vite-plugin-top-level-await": "^1.6.0",
		"vite-plugin-wasm": "^3.5.0",
		"zod": "^4.2.1"
	}
}
